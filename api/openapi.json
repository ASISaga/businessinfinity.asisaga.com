        "/agents/{agent_role}/ask": {
            "post": {
                "summary": "Ask an agent a question",
                "operationId": "askAgent",
                "description": "Send a query to a specific C-Suite agent and receive a response via AOS Service Bus",
                "tags": [
                    "agents",
                    "aos"
                ],
                "parameters": [
                    {
                        "name": "agent_role",
                        "in": "path",
                        "required": true,
                        "description": "Agent role (CEO, CFO, CTO, CMO, COO, CHRO, CSO)",
                        "schema": {
                            "type": "string",
                            "enum": [
                                "CEO",
                                "CFO",
                                "CTO",
                                "CMO",
                                "COO",
                                "CHRO",
                                "CSO"
                            ]
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "required": [
                                    "message"
                                ],
                                "properties": {
                                    "message": {
                                        "type": "string",
                                        "description": "Query message for the agent"
                                    },
                                    "context": {
                                        "type": "object",
                                        "description": "Optional context for the query"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Agent response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "agent": {
                                            "type": "string"
                                        },
                                        "response": {
                                            "type": "string"
                                        },
                                        "via": {
                                            "type": "string",
                                            "enum": [
                                                "servicebus",
                                                "local"
                                            ]
                                        },
                                        "timestamp": {
                                            "type": "string",
                                            "format": "date-time"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },

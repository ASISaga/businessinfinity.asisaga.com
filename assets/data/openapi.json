        "/api/decisions": {
            "post": {
                "summary": "Create strategic decision",
                "operationId": "createStrategicDecision",
                "description": "Submit a strategic decision for multi-agent collaboration and voting",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "required": [
                                    "type",
                                    "context"
                                ],
                                "properties": {
                                    "type": {
                                        "type": "string",
                                        "enum": [
                                            "strategic",
                                            "financial",
                                            "technical",
                                            "operational"
                                        ],
                                        "description": "Type of decision"
                                    },
                                    "context": {
                                        "type": "string",
                                        "description": "Decision context and parameters"
                                    },
                                    "stakeholders": {
                                        "type": "array",
                                        "items": {
                                            "type": "string"
                                        },
                                        "description": "Optional list of agent roles to involve (CEO, CFO, etc.)"
                                    },
                                    "params": {
                                        "type": "object",
                                        "description": "Additional decision parameters"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Decision created and queued",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "decision_id": {
                                            "type": "string",
                                            "format": "uuid"
                                        },
                                        "status": {
                                            "type": "string",
                                            "enum": [
                                                "queued",
                                                "completed",
                                                "requires_review"
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/workflows/{workflow_name}": {
            "post": {
                "summary": "Execute business workflow",
                "operationId": "executeWorkflow",
                "description": "Execute a predefined business workflow (e.g., product_launch, funding_round)",
                "parameters": [
                    {
                        "name": "workflow_name",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "description": "Name of workflow to execute (product_launch, funding_round, market_analysis)"
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "params": {
                                        "type": "object",
                                        "description": "Workflow-specific parameters"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Workflow execution result",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "execution_id": {
                                            "type": "string"
                                        },
                                        "status": {
                                            "type": "string"
                                        },
                                        "result": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/agents": {
            "get": {
                "summary": "List all available agents",
                "operationId": "getAgents",
